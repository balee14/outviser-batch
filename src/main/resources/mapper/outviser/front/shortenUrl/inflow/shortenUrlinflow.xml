<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.enliple.outviserbatch.outviser.front.shortenUrl.inflow.mapper.ShortenUrlInflowMapper"> 
    
    <!-- -->
    <insert id="insertShortenUrlInflowLog" parameterType="dataMap">
        /*enliple.ibot.outviser.front.urlshortener2.insertShortenUrlInflowLog*/
        INSERT INTO OV_SHORTEN_URL_INFLOW_LOG (
             SHORTEN_URL_CODE
            ,IP_ADDR
            ,USER_AGENT
        ) VALUES (
             #{code}
            ,#{connectIp}
            ,#{userAgent}
        );      
    </insert>
    
    
    
</mapper>